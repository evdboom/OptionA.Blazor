@namespace OptionA.Blazor.Blog
@if (Content is not null)
{
    @switch (Content.ListType)
{
    case ListType.OrderedList:
        <ol @attributes="@Content.GetAttributes(DataProvider)">
                <OptAListContent Content="@Content"/>
            </ol>
        break;
    case ListType.UnorderedList:
        <ul @attributes="@Content.GetAttributes(DataProvider)">
                <OptAListContent Content="@Content" />
            </ul>
        break;
}
}


